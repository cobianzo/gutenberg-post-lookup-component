{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "coco/test-block",
	"version": "1.0.0",
	"title": "Coco Test Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "A test block for the Coco Post Lookup plugin",
	"supports": {
		"html": false
	},
	"attributes": {
		"postID": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "coco",
	"editorScript": "file:../../build/test-block.js",
	"editorStyle": "file:../../build/style-test-block.css",
	"style": "file:../../build/style-test-block.css",
	"render": "file:./render.php"
}
